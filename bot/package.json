{
    "name": "caniventure-bot",
    "private": true,
    "version": "0.0.1",
    "description": "A caniballistic Discord bot",
    "scripts": {
        "start": "ts-node ./src/index.ts",
        "tsc": "tsc",
        "enable-scripts": "chmod +x ./scripts/* && echo \"Done\"",
        "create-command:unix": "./scripts/create-command.sh --command",
        "create-command-group:unix": "./scripts/create-command.sh --group",
        "create-command:win": "powershell -Command \".\\scripts\\create-command.ps1 --command\"",
        "create-command-group:win": "powershell -Command \".\\scripts\\create-command.ps1 --group\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Caniventure-Development/Caniventure.git"
    },
    "author": "Caniventure Development",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Caniventure-Development/Caniventure/issues"
    },
    "homepage": "https://github.com/Caniventure-Development/Caniventure#readme",
    "dependencies": {
        "@discordjs/builders": "^1.7.0",
        "canvas": "^2.11.2",
        "discord.js": "^14.14.1",
        "dotenv": "^16.4.5",
        "mongoose": "^8.2.2",
        "picocolors": "^1.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.11.16",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}
